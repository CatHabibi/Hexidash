<!doctype html>
<html lang="en" dir="ltr">

<head>

    <!-- META DATA -->
    <meta charset="UTF-8">
    <meta name='viewport' content='width=device-width, initial-scale=1.0, user-scalable=0'>
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="keywords"
        content="admin,admin dashboard,admin panel,admin template,bootstrap,clean,dashboard,flat,jquery,modern,responsive,premium admin templates,responsive admin,ui,ui kit.">

    <!-- FAVICON -->
    <link rel="shortcut icon" type="image/x-icon" href="../assets/images/brand/favicon.ico" />

    <!-- TITLE -->
    <title>Administrator - Reliactyl</title>

    <!-- BOOTSTRAP CSS -->
    <link id="style" href="../assets/plugins/bootstrap/css/bootstrap.min.css" rel="stylesheet" />

    <!-- STYLE CSS -->
    <link href="../assets/css/style.css" rel="stylesheet" />
    <link href="../assets/css/dark-style.css" rel="stylesheet" />
    <link href="../assets/css/transparent-style.css" rel="stylesheet">
    <link href="../assets/css/skin-modes.css" rel="stylesheet" />

    <!--- FONT-ICONS CSS -->
    <link href="../assets/css/icons.css" rel="stylesheet" />

    <!-- COLOR SKIN CSS -->
    <link id="theme" rel="stylesheet" type="text/css" media="all" href="../assets/colors/color1.css" />

</head>

<body class="app sidebar-mini ltr dark-mode">

    <!-- GLOBAL-LOADER -->
    <div id="global-loader">
        <img src="../assets/images/loader.svg" class="loader-img" alt="Loader">
    </div>
    <!-- /GLOBAL-LOADER -->

    <!-- PAGE -->
    <div class="page">

        <%- include('./parts/header') %>
            <%- include('./parts/sidebar') %>

                <!--app-content open-->
                <div class="main-content app-content mt-0">
                    <div class="side-app">

                        <!-- CONTAINER -->
                        <div class="main-container container-fluid">

                            <!-- PAGE-HEADER -->
                            <div class="page-header">
                                <h1 class="page-title">Administrator</h1>
                            </div>
                            <!-- PAGE-HEADER END -->
                            <% if (variables) { %>
                                <% if (variables.error && status_replies.error[variables.error]) { %>
                                    <div class="alert alert-danger" role="alert">
                                        <button type="button" class="btn-close" data-bs-dismiss="alert"
                                            aria-hidden="true">×</button>
                                        <i class="fa fa-frown-o me-2" aria-hidden="true"></i>
                                        <%- eval(`\`${status_replies.error[variables.error]}\``) %>
                                    </div>
                                    <% } else if (variables.success && status_replies.success[variables.success]) { %>
                                        <div class="alert alert-success" role="alert">
                                            <button type="button" class="btn-close" data-bs-dismiss="alert"
                                                aria-hidden="true">×</button>
                                            <i class="fa fa-check-circle-o me-2" aria-hidden="true"></i>
                                            <%- eval(`\`${status_replies.success[variables.success]}\``) %>
                                        </div>
                                        <% } else { %>
                                            <div class="alert alert-info" role="alert">
                                                <button type="button" class="btn-close" data-bs-dismiss="alert"
                                                    aria-hidden="true">×</button>
                                                <i class="fa fa-bell-o me-2" aria-hidden="true"></i>
                                                <%= JSON.stringify(variables) %>
                                            </div>
                                            <% } %>
                                                <% } %>
                                                    <!-- ROW-1 -->
                                                    <div class="row">
                                                        <div class="col-lg-12 col-md-12 col-sm-12 col-xl-12">
                                                            <div class="row">
                                                                <div class="col-lg-6 col-md-12 col-sm-12 col-xl-3">
                                                                    <div class="card overflow-hidden">
                                                                        <div class="card-body">
                                                                            <div class="d-flex">
                                                                                <div class="mt-2">
                                                                                    <h6 class="">CPU</h6>
                                                                                    <h2 class="mb-0 number-font">
                                                                                        <%= current.cpu %>% / <%=
                                                                                                total.cpu %>%
                                                                                    </h2>
                                                                                </div>
                                                                            </div>
                                                                        </div>
                                                                    </div>
                                                                </div>
                                                                <div class="col-lg-6 col-md-12 col-sm-12 col-xl-3">
                                                                    <div class="card overflow-hidden">
                                                                        <div class="card-body">
                                                                            <div class="d-flex">
                                                                                <div class="mt-2">
                                                                                    <h6 class="">MEMORY</h6>
                                                                                    <h2 class="mb-0 number-font">
                                                                                        <%= current.memory %>MB / <%=
                                                                                                total.memory %>MB
                                                                                    </h2>
                                                                                </div>
                                                                            </div>
                                                                        </div>
                                                                    </div>
                                                                </div>
                                                                <div class="col-lg-6 col-md-12 col-sm-12 col-xl-3">
                                                                    <div class="card overflow-hidden">
                                                                        <div class="card-body">
                                                                            <div class="d-flex">
                                                                                <div class="mt-2">
                                                                                    <h6 class="">SERVERS</h6>
                                                                                    <h2 class="mb-0 number-font">
                                                                                        <%= current.servers %> server<%=
                                                                                                current.servers==1 ? ""
                                                                                                : "s" %>
                                                                                                / <%= total.servers %>
                                                                                                    server<%=
                                                                                                        total.servers==1
                                                                                                        ? "" : "s" %>
                                                                                    </h2>
                                                                                </div>
                                                                            </div>
                                                                        </div>
                                                                    </div>
                                                                </div>
                                                                <div class="col-lg-6 col-md-12 col-sm-12 col-xl-3">
                                                                    <div class="card overflow-hidden">
                                                                        <div class="card-body">
                                                                            <div class="d-flex">
                                                                                <div class="mt-2">
                                                                                    <h6 class="">DISK</h6>
                                                                                    <h2 class="mb-0 number-font">
                                                                                        <%= current.disk %>MB / <%=
                                                                                                total.disk %>MB
                                                                                    </h2>
                                                                                </div>
                                                                            </div>
                                                                        </div>
                                                                    </div>
                                                                </div>

                                                            </div>
                                                        </div>
                                                    </div>
                                                    <!-- ROW-1 END -->
                                                    <div class="row">
                                                        <div class="card">
                                                            <div class="card-body">
                                                                <h3>Administrator Options</h3>
                                                            </div>
                                                        </div>
                                                        <div class="card">
                                                            <div class="card-header">
                                                                <div class="card-title">User Info</div>
                                                            </div>
                                                            <div class="card-body">
                                                                <p>
                                                                    <label>Discord ID:</label>
                                                                    <input class="form-control" id="userinfo_id"><br>
                                                                    <button class="btn btn-primary"
                                                                        onclick="asyncAlert(userInfo(document.getElementById('userinfo_id').value))">Get</button>
                                                                    <button class="btn btn-danger"
                                                                        onclick="asyncAlert(blacklistUser(document.getElementById('userinfo_id').value))">Blacklist</button>
                                                                    <button class="btn btn-info"
                                                                        onclick="asyncAlert(unblacklistUser(document.getElementById('userinfo_id').value))">Unblacklist</button>
                                                                </p>
                                                            </div>
                                                        </div>
                                                        <div class="card">
                                                            <div class="card-header">
                                                                <div class="card-title">Set Coins</div>
                                                            </div>
                                                            <div class="card-body">
                                                                <p>
                                                                    <label>Discord ID:</label>
                                                                    <input class="form-control" id="setcoins_id">
                                                                    <label>Coins:</label>
                                                                    <input class="form-control" id="setcoins_coins"><br>
                                                                    <button class="btn btn-info"
                                                                        onclick="asyncAlert(setCoins(document.getElementById('setcoins_id').value, parseFloat(document.getElementById('setcoins_coins').value), true))">Add</button>
                                                                    <button class="btn btn-info"
                                                                        onclick="asyncAlert(setCoins(document.getElementById('setcoins_id').value, parseFloat(document.getElementById('setcoins_coins').value)))">Set</button>
                                                                </p>
                                                            </div>
                                                        </div>
                                                        <div class="card">
                                                            <div class="card-header">
                                                                <div class="card-title">Set Package</div>
                                                            </div>
                                                            <div class="card-body">
                                                                <p>
                                                                    <label>Discord ID:</label>
                                                                    <input class="form-control" id="setpackage_id">
                                                                    <label>Package:</label>
                                                                    <select
                                                                        class="btn btn-block btn-outline-default dropdown-toggle"
                                                                        id="setpackage_package">
                                                                        <option value="">Default package. (ID: <%=
                                                                                settings.packages.default %>)</option>
                                                                        <% for (let [ id, info ] of
                                                                            Object.entries(settings.packages.list)) { %>
                                                                            <option value="<%= id %>">
                                                                                <%= info.display %> (ID: <%= id %>) |
                                                                                        Memory: <%= info.memory %>MB,
                                                                                            Disk: <%= info.disk %>, CPU:
                                                                                                <%= info.cpu %>,
                                                                                                    Servers: <%=
                                                                                                        info.servers %>
                                                                            </option>
                                                                            <% } %>
                                                                    </select><br>
                                                                    <button class="btn btn-info"
                                                                        onclick="asyncAlert(setPackage(document.getElementById('setpackage_id').value, document.getElementById('setpackage_package').value.length == 0 ? null : document.getElementById('setpackage_package').value))">Set</button>
                                                                </p>
                                                            </div>
                                                        </div>
                                                        <div class="card">
                                                            <div class="card-header">
                                                                <div class="card-title">Set extra resources</div>
                                                            </div>
                                                            <div class="card-body">
                                                                <p>Be careful when using this tool. The user might lose
                                                                    their extra resources from leaving a J4R server.
                                                                    Since resources can't become negatives, if their
                                                                    resources is supposed to go under zero, their
                                                                    resources will just be set to 0.</p>
                                                                <p>
                                                                    <label>Discord ID:</label>
                                                                    <input class="form-control" id="setresources_id">
                                                                    <label>Memory:</label>
                                                                    <input class="form-control"
                                                                        id="setresources_memory">
                                                                    <label>Disk:</label>
                                                                    <input class="form-control" id="setresources_disk">
                                                                    <label>CPU:</label>
                                                                    <input class="form-control" id="setresources_cpu">
                                                                    <label>Servers:</label>
                                                                    <input class="form-control"
                                                                        id="setresources_servers"><br>
                                                                    <button class="btn btn-info"
                                                                        onclick="asyncAlert(setResources(document.getElementById('setresources_id').value, parseFloat(document.getElementById('setresources_memory').value), parseFloat(document.getElementById('setresources_disk').value), parseFloat(document.getElementById('setresources_cpu').value), parseFloat(document.getElementById('setresources_servers').value), true))">Add</button>
                                                                    <button class="btn btn-info"
                                                                        onclick="asyncAlert(setResources(document.getElementById('setresources_id').value, parseFloat(document.getElementById('setresources_memory').value), parseFloat(document.getElementById('setresources_disk').value), parseFloat(document.getElementById('setresources_cpu').value), parseFloat(document.getElementById('setresources_servers').value)))">Set</button>
                                                                </p>
                                                            </div>
                                                        </div>
                                                        <div class="card">
                                                            <div class="card-header">
                                                                <div class="card-title">Create Coupon</div>
                                                            </div>
                                                            <div class="card-body">
                                                                <p>
                                                                    <label>Code:</label>
                                                                    <input class="form-control" id="createcoupon_code">
                                                                    <label>Coins:</label>
                                                                    <input class="form-control" id="createcoupon_coins">
                                                                    <label>Memory:</label>
                                                                    <input class="form-control"
                                                                        id="createcoupon_memory">
                                                                    <label>Disk:</label>
                                                                    <input class="form-control" id="createcoupon_disk">
                                                                    <label>CPU:</label>
                                                                    <input class="form-control" id="createcoupon_cpu">
                                                                    <label>Servers:</label>
                                                                    <input class="form-control"
                                                                        id="createcoupon_servers"><br>
                                                                    <button class="btn btn-info"
                                                                        onclick="asyncAlert(createCoupon(document.getElementById('createcoupon_code').value, parseFloat(document.getElementById('createcoupon_coins').value), parseFloat(document.getElementById('createcoupon_memory').value), parseFloat(document.getElementById('createcoupon_disk').value), parseFloat(document.getElementById('createcoupon_cpu').value), parseFloat(document.getElementById('createcoupon_servers').value)))">Create</button>
                                                                </p>
                                                            </div>
                                                        </div>
                                                        <div class="card">
                                                            <div class="card-header">
                                                                <div class="card-title">Revoke Coupon</div>
                                                            </div>
                                                            <div class="card-body">
                                                                <p>
                                                                    <label>Code:</label>
                                                                    <input class="form-control"
                                                                        id="revokecoupon_code"><br>
                                                                    <button class="btn btn-info"
                                                                        onclick="asyncAlert(revokeCoupon(document.getElementById('revokecoupon_code').value));">Revoke</button>
                                                                </p>
                                                            </div>
                                                        </div>
                                                        <div class="card">
                                                            <div class="card-header">
                                                                <div class="card-title">Create Join 4 resources</div>
                                                            </div>
                                                            <div class="card-body">
                                                                <p>You cannot modify or remove a J4R after it has been
                                                                    made. Make sure you have every information correct
                                                                    before creating the J4R.<br>
                                                                    Keep in mind you can have multiple J4Rs on the same
                                                                    server.<br>
                                                                    Users will need to click "Update user information."
                                                                    after joining the corresponding J4R server.</p>
                                                                <p>
                                                                    <label>J4R ID:</label>
                                                                    <input class="form-control" id="createj4r_j4rid">
                                                                    <label>Discord Server ID:</label>
                                                                    <input class="form-control" id="createj4r_serverid">
                                                                    <label>Expires on X days:</label>
                                                                    <input class="form-control"
                                                                        id="createj4r_expiresonindays">
                                                                    <label>Memory:</label>
                                                                    <input class="form-control" id="createj4r_memory">
                                                                    <label>Disk:</label>
                                                                    <input class="form-control" id="createj4r_disk">
                                                                    <label>CPU:</label>
                                                                    <input class="form-control" id="createj4r_cpu">
                                                                    <label>Servers:</label>
                                                                    <input class="form-control"
                                                                        id="createj4r_servers"><br>
                                                                    <button class="btn btn-info"
                                                                        onclick="asyncAlert(createJ4R(document.getElementById('createj4r_j4rid').value, document.getElementById('createj4r_serverid').value, parseFloat(document.getElementById('createj4r_expiresonindays').value), parseFloat(document.getElementById('createj4r_memory').value), parseFloat(document.getElementById('createj4r_disk').value), parseFloat(document.getElementById('createj4r_cpu').value), parseFloat(document.getElementById('createj4r_servers').value)));">Create</button>
                                                                </p>
                                                            </div>
                                                        </div>
                                                        <script>
                                                            async function asyncAlert(func) {
                                                                alert(JSON.stringify(await func));
                                                            };
                                                        </script>

                                                        <script src="/assets/js/admin.js"></script>
                                                    </div>

                        </div>
                        <!-- CONTAINER END -->
                    </div>
                </div>
                <!--app-content close-->

    </div>


    <%- include('./parts/footer') %>

        </div>

        <!-- BACK-TO-TOP -->
        <a href="#top" id="back-to-top"><i class="fa fa-angle-up"></i></a>

        <!-- JQUERY JS -->
        <script src="../assets/js/jquery.min.js"></script>

        <!-- BOOTSTRAP JS -->
        <script src="../assets/plugins/bootstrap/js/popper.min.js"></script>
        <script src="../assets/plugins/bootstrap/js/bootstrap.min.js"></script>

        <!-- SPARKLINE JS-->
        <script src="../assets/js/jquery.sparkline.min.js"></script>

        <!-- Sticky js -->
        <script src="../assets/js/sticky.js"></script>

        <!-- CHART-CIRCLE JS-->
        <script src="../assets/js/circle-progress.min.js"></script>

        <!-- PIETY CHART JS-->
        <script src="../assets/plugins/peitychart/jquery.peity.min.js"></script>
        <script src="../assets/plugins/peitychart/peitychart.init.js"></script>

        <!-- SIDEBAR JS -->
        <script src="../assets/plugins/sidebar/sidebar.js"></script>

        <!-- Perfect SCROLLBAR JS-->
        <script src="../assets/plugins/p-scroll/perfect-scrollbar.js"></script>
        <script src="../assets/plugins/p-scroll/pscroll.js"></script>
        <script src="../assets/plugins/p-scroll/pscroll-1.js"></script>

        <!-- INTERNAL CHARTJS CHART JS-->
        <script src="../assets/plugins/chart/Chart.bundle.js"></script>
        <script src="../assets/plugins/chart/rounded-barchart.js"></script>
        <script src="../assets/plugins/chart/utils.js"></script>

        <!-- INTERNAL SELECT2 JS -->
        <script src="../assets/plugins/select2/select2.full.min.js"></script>

        <!-- INTERNAL Data tables js-->
        <script src="../assets/plugins/datatable/js/jquery.dataTables.min.js"></script>
        <script src="../assets/plugins/datatable/js/dataTables.bootstrap5.js"></script>
        <script src="../assets/plugins/datatable/dataTables.responsive.min.js"></script>

        <!-- INTERNAL APEXCHART JS -->
        <script src="../assets/js/apexcharts.js"></script>
        <script src="../assets/plugins/apexchart/irregular-data-series.js"></script>

        <!-- C3 CHART JS -->
        <script src="../assets/plugins/charts-c3/d3.v5.min.js"></script>
        <script src="../assets/plugins/charts-c3/c3-chart.js"></script>

        <!-- CHART-DONUT JS -->
        <script src="../assets/js/charts.js"></script>

        <!-- INTERNAL Flot JS -->
        <script src="../assets/plugins/flot/jquery.flot.js"></script>
        <script src="../assets/plugins/flot/jquery.flot.fillbetween.js"></script>
        <script src="../assets/plugins/flot/chart.flot.sampledata.js"></script>
        <script src="../assets/plugins/flot/dashboard.sampledata.js"></script>

        <!-- INTERNAL Vector js -->
        <script src="../assets/plugins/jvectormap/jquery-jvectormap-2.0.2.min.js"></script>
        <script src="../assets/plugins/jvectormap/jquery-jvectormap-world-mill-en.js"></script>

        <!-- SIDE-MENU JS-->
        <script src="../assets/plugins/sidemenu/sidemenu.js"></script>

        <!-- INTERNAL INDEX JS -->
        <script src="../assets/js/index1.js"></script>

        <!-- Color Theme js -->
        <script src="../assets/js/themeColors.js"></script>

        <!-- CUSTOM JS -->
        <script src="../assets/js/custom.js"></script>

</body>

</html>